    <script>
        let date = new Date();
        document.getElementById('report1_date').innerHTML = date.getDate() + '/' + date.getMonth() + '/' + date.getFullYear();
        document.getElementById('report1_time').innerHTML = date.getHours() + ' : ' + date.getMinutes();
    </script>
    <script type="text/jsx" src="/js/custom.jsx"></script>
    <script src="/js/app.js"></script>
    <!-- <script type="text/jsx" src="/js/testRactTable.jsx"></script> -->
</body>
